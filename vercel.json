{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/server.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  },
  "installCommand": "pip install -r backend/requirements.txt"
}
